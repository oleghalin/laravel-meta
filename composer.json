{
    "name": "khalin/laravel-meta",
    "description": "Flexible Meta Tags for Laravel Application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Khalin",
            "email": "oleghalinua@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0",
        "orchestra/testbench": "~3.8.0"
    },
    "autoload": {
        "psr-4": {
            "Khalin\\Meta\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Khalin\\Meta\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Khalin\\Meta\\MetaTagsServiceProvider"
            ]
        }
    }
}
